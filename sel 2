package main

import "fmt"

func main() {
	var num int
	println("digite um numero")
	fmt.Scan(&num)
	var num1 int
	println("digite outro numero")
	fmt.Scan(&num1)
	var num2 int
	println("digite outro numero")
	fmt.Scan(&num2)
	if num < num1 && num < num2 {
		println("seu numero é", num)
	}
	if num1 < num && num1 < num2 {
		println("seu numero é", num1)
	}
	if num2 < num && num2 < num1 {
		println("seu numero é", num2)
	}
}
