package main

import "fmt"

func main() {
	var num int
	fmt.Println("escrava sua idade")
	fmt.Scan(&num)
	if num <= 9 {
		fmt.Println("Mirim")
	} else if num >= 10 && num <= 13 {
		fmt.Println("Infantil")

	} else if num >= 14 && num <= 17 {
		fmt.Println("Juvenil")

	} else if num >= 18 {
		fmt.Println("Adulto")
	}
}
